<volume>
  <name>{{ .Name }}</name>
  <allocation>0</allocation>
  <capacity unit="G">{{ .DiskSpaceGB }}</capacity>
  <target>
    <format type="qcow2"/>
    <path>/var/lib/virt/images/{{ .Name }}.img</path>
  </target>
  <backingStore>
    <path>{{ .Image }}</path>
    <format type="qcow2"/>
  </backingStore>
</volume>
