<volume>
  <name>test.img</name>
  <!-- spare allocation -->
  <allocation>0</allocation>
  <!-- 100G capacity -->
  <capacity unit="G">100</capacity>
  <target>
    <format type="qcow2"/>
    <path>/var/lib/virt/images/test.img</path>
  </target>
  <backingStore>
    <path>xenial-server-cloudimg-amd64-disk1.img</path>
    <format type="qcow2"/>
  </backingStore>
</volume>
